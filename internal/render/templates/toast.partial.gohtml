<script>
    function notify(msg, msgType) {
        Swal.fire({
            position: "top-end",
            icon: msgType,
            title: msg,
            showConfirmButton: false,
            timer: 2000,
            toast: true
        });
    }

    {{with .Success}}
    notify("{{.}}", "success")
    {{end}}
    {{with .Warning}}
    notify("{{.}}", "warning")
    {{end}}

    {{with .Error}}
    notify("{{.}}", "error")
    {{end}}
</script>